<div class='container'>
  <%=  link_to "New project", new_project_path, class: "btn btn-info pull-right margin-top-button"%>
  <div class='page-header'>
    <h1>Projects</h1>

  </div>

  <table class="table">
    <thead>
      <tr>
        <th>
          Name
        </th>
      </tr>
    </thead>

    <tbody>
      <% @projects.each do |project| %>
      <tr>
        <td>
          <%= link_to project.name, project_path(project) %>
          <% end %>
        </td>
  </table>
  
</div>
